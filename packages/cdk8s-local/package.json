{
	"name": "cdk8s-local",
	"version": "0.1.0",
	"type": "module",
	"private": false,
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"lint": "eslint . --max-warnings 0",
		"typecheck": "tsc --noEmit",
		"dist": "bunup",
		"generate-config-types": "bun run ./scripts/generate-config-types.ts"
	},
	"dependencies": {
		"arktype": "catalog:",
		"cdk8s": "catalog:",
		"cdk8s-kbld2": "workspace:*",
		"cmd-ts": "catalog:",
		"constructs": "catalog:",
		"is-wsl": "^3.1.0",
		"@repo/utils": "workspace:*"
	},
	"devDependencies": {
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@types/bun": "catalog:",
		"eslint": "catalog:",
		"json-schema-to-typescript": "^15.0.4",
		"typescript": "catalog:",
		"bunup": "catalog:"
	},
	"exports": {
		".": {
			"bun": "./src/index.ts",
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./*": {
			"bun": "./src/*.ts",
			"require": {
				"types": "./dist/*.d.cts",
				"default": "./dist/*.cjs"
			},
			"import": {
				"types": "./dist/*.d.ts",
				"default": "./dist/*.js"
			}
		},
		"./package.json": "./package.json"
	}
}
