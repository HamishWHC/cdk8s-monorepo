{
	"name": "cdk8s-opinionated-cli",
	"version": "0.1.0",
	"private": false,
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"lint": "eslint . --max-warnings 0",
		"typecheck": "tsc --noEmit",
		"dist": "tsup",
		"generate-config-types": "bun run ./scripts/generate-config-types.ts"
	},
	"dependencies": {
		"cdk8s": "catalog:",
		"cdk8s-kbld2": "workspace:*",
		"cdk8s-local": "workspace:*",
		"cmd-ts": "catalog:",
		"constructs": "catalog:",
		"@repo/utils": "workspace:*"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.56.2",
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@types/bun": "catalog:",
		"ajv": "^8.17.1",
		"eslint": "catalog:",
		"tsup": "catalog:",
		"typescript": "catalog:"
	},
	"exports": {
		".": {
			"bun": "./src/index.ts",
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./*": {
			"bun": "./src/*.ts",
			"require": {
				"types": "./dist/*.d.cts",
				"default": "./dist/*.cjs"
			},
			"import": {
				"types": "./dist/*.d.ts",
				"default": "./dist/*.js"
			}
		},
		"./package.json": "./package.json"
	}
}
