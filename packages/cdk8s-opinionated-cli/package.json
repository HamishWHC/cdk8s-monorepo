{
	"name": "cdk8s-opinionated-cli",
	"version": "0.2.0",
	"private": false,
	"type": "module",
	"scripts": {
		"lint": "eslint . --max-warnings 0",
		"typecheck": "tsc --noEmit",
		"dist": "bun --bun tsdown",
		"publish-pkg": "bun publish --tolerate-republish"
	},
	"dependencies": {
		"cdk8s": "catalog:",
		"cdk8s-kbld2": "workspace:*",
		"cdk8s-local": "workspace:*",
		"cmd-ts": "catalog:",
		"constructs": "catalog:",
		"@hamishwhc/cdk8s-monorepo-utils": "workspace:*"
	},
	"devDependencies": {
		"@hamishwhc/cdk8s-monorepo-eslint-config": "workspace:*",
		"@hamishwhc/cdk8s-monorepo-typescript-config": "workspace:*",
		"@types/bun": "catalog:",
		"eslint": "catalog:",
		"tsdown": "catalog:",
		"typescript": "catalog:"
	},
	"exports": {
		".": {
			"bun": "./src/index.ts",
			"default": "./dist/index.mjs"
		},
		"./package.json": "./package.json"
	}
}
