{
	"name": "@hamishwhc/cdk8s-monorepo-utils",
	"version": "0.2.0",
	"private": false,
	"type": "module",
	"scripts": {
		"lint": "eslint . --max-warnings 0",
		"typecheck": "tsc --noEmit",
		"dist": "bun --bun tsdown",
		"publish-pkg": "bun publish --tolerate-republish --access public"
	},
	"dependencies": {
		"cmd-ts": "catalog:",
		"tslog": "^4.10.2"
	},
	"devDependencies": {
		"@hamishwhc/cdk8s-monorepo-eslint-config": "workspace:*",
		"@hamishwhc/cdk8s-monorepo-typescript-config": "workspace:*",
		"tsdown": "catalog:"
	},
	"exports": {
		".": {
			"bun": "./src/index.ts",
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"./*": {
			"bun": "./src/*.ts",
			"types": "./dist/*.d.mts",
			"default": "./dist/*.mjs"
		},
		"./package.json": "./package.json"
	}
}
