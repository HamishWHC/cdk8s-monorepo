{
	"name": "@repo/utils",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"lint": "eslint . --max-warnings 0",
		"typecheck": "tsc --noEmit",
		"dist": "bun --bun tsdown"
	},
	"dependencies": {
		"tslog": "^4.10.2"
	},
	"exports": {
		".": {
			"bun": "./src/index.ts",
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"./*": {
			"bun": "./src/*.ts",
			"types": "./dist/*.d.mts",
			"default": "./dist/*.mjs"
		},
		"./package.json": "./package.json"
	},
	"devDependencies": {
		"tsdown": "catalog:"
	}
}
