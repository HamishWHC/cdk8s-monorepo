{
	"name": "cdk-typed-context",
	"version": "0.1.0",
	"type": "module",
	"private": false,
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"lint": "eslint . --max-warnings 0",
		"typecheck": "tsc --noEmit",
		"dist": "tsup"
	},
	"devDependencies": {
		"constructs": "catalog:",
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@repo/utils": "workspace:*",
		"tsup": "catalog:",
		"eslint": "catalog:",
		"typescript": "catalog:"
	},
	"exports": {
		".": {
			"bun": "./src/index.ts",
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./*": {
			"bun": "./src/*.ts",
			"require": {
				"types": "./dist/*.d.cts",
				"default": "./dist/*.cjs"
			},
			"import": {
				"types": "./dist/*.d.ts",
				"default": "./dist/*.js"
			}
		},
		"./package.json": "./package.json"
	}
}
