{
	"$schema": "./node_modules/turbo/schema.json",
	"ui": "tui",
	"tasks": {
		"lint": {
			"dependsOn": ["^lint"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"dist": {
			"dependsOn": ["^dist"],
			"outputs": ["dist/**"]
		},
		"publish-pkg": {
			"dependsOn": ["^dist", "dist", "^publish-pkg"],
			"cache": false
		}
	}
}
